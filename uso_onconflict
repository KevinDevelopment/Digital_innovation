CREATE TABLE IF NOT EXISTS teste (
	id SERIAL PRIMARY KEY,
	nome VARCHAR(50) NOT NULL,
	created_at TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP
	
);

DROP TABLE IF EXISTS TESTE;

CREATE TABLE IF NOT EXISTS teste (
	cpf VARCHAR(11) NOT NULL,
	nome VARCHAR(50) NOT NULL,
	created_at TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	PRIMARY KEY (cpf)
);

INSERT INTO teste (cpf,nome,created_at) VALUES ('12354687498','kEVIN','2019-07-01 12:00:00');

INSERT INTO teste (cpf,nome,created_at) VALUES ('12354687498','kEVIN','2019-07-01 12:00:00')
ON CONFLICT (cpf) DO NOTHING;/* USANDO NO CONFLICT PARA EVITAR ERRO DE PRIMARY KEY */

UPDATE teste SET nome = 'Kevin Ferreira' WHERE cpf = '12354687498';

SELECT * FROM teste;
